{
  "name": "vue-ssr-cowbell",
  "version": "1.0.0",
  "description": "Cowbell chart with Vue SSR",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "npm run build && node server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "rollup -c rollup.config.js --environment CLIENT",
    "build:server": "rollup -c rollup.config.js --environment SERVER",
    "start": "node server.js"
  },
  "keywords": [
    "vue",
    "ssr",
    "rollup",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vue/server-renderer": "^3.4.27",
    "express": "^4.19.2",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitejs/plugin-vue": "^5.0.5",
    "postcss": "^8.4.38",
    "rollup": "^4.18.0",
    "rollup-plugin-css-only": "^4.5.2"
  }
}
